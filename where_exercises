USE employees;

SELECT emp_no, first_name, last_name
FROM employees
WHERE first_name IN ('Irena' 'Vidya' 'Maya');

SELECT emp_no, first_name, last_name
FROM employees
WHERE last_name = 'E';

SELECT * FROM employees WHERE hire_date = '1990' '1999';

SELECT * FROM employees WHERE birth_date = '12-25';

